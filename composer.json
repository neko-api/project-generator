{
    "name": "neko-api/project-generator",
    "type": "tool",
    "description": "NekoAPI ProjectGenerator Tool",
    "keywords": ["project-generator", "neko-api"],
    "homepage": "https://github.com/neko-api/project-generator",
    "license": "MIT",
    "authors": [
        {
            "name": "Aurimas Niekis",
            "email": "aurimas@niekis.lt"
        }
    ],
    "require": {
        "php": ">=7.0",
        "cpliakas/git-wrapper": "^1.6",
        "symfony/console": ">=3.1",
        "symfony/finder": ">=3.1",
        "symfony/filesystem": ">=3.1",
        "funct/funct": "^1.3"
    },
    "bin": ["bin/nekoapi-project"],
    "autoload": {
        "psr-4": { "NekoAPI\\Tool\\ProjectGenerator\\": "src" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
